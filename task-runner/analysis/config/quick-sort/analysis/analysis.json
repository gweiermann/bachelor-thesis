{
    "collect": [
        {
            "type": "arrayWatcher",
            "parameters": {
                "name": "arr",
                "size": "n"
            },
            "key": "array"
        },
        {
            "type": "currentLine",
            "key": "line"
        },
        {
            "type": "currentScope",
            "key": "scope"
        },
        {
            "type": "recursionWatcher",
            "key": "recursion"
        },
        {
            "type": "compareOperationWatcher",
            "key": "comparison"
        }
    ],
    "postProcess": [
        {
            "type": "skipInterSwappingSteps",
            "parameters": {
                "array": "array"
            },
            "key": "skippedStep"
        },
        {
            "type": "keepTrackOfItems",
            "parameters": {
                "array": "array"
            },
            "key": "event"
        }
    ]
}